{
	"compilerOptions": {
		"incremental": true,
		"target": "es2016",
		"lib": [
			"ES2016",
			"DOM",
			"ES2016.Array.Include",
			"ES2015.Collection",
			"ES2015.Iterable"
		],
		"module": "ES6",
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"strict": true,
		"skipLibCheck": true
	}
}
